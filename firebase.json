{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/**.html",
      "destination": "/DIRECTORY/**.html"
      }
    ],
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "redirects": [ {
      "source": "/:page*",
      "destination": "http://https://web-locker-a1d76.firebaseapp.com/:page",
      "type": 301
    }]
  }
}